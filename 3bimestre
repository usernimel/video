import shutil

# Caminho da imagem gerada
image_path = "/mnt/data/A_cute_kitten_sitting_on_a_soft_pillow,_with_fluff.png"

# Caminho para mover a imagem para uma nova pasta acessÃ­vel
new_image_path = "/mnt/data/kitten_image.png"

# Movendo a imagem para a nova pasta
shutil.move(image_path, new_image_path)

# Gerando o link de acesso
new_image_path
